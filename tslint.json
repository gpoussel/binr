{
  "extends": ["tslint:latest", "tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "prettier": true,
    "no-bitwise": false,
    "object-literal-sort-keys": false,
    "variable-name": {
      "options": ["ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"]
    }
  },
  "linterOptions": {
    "exclude": ["**/node_modules/**/*"]
  }
}
