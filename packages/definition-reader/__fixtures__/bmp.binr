#name "BMP Format"
#extension ".bmp"

struct FileHeader {
    uint:16    type; 
    uint:32    size; 
    uint:16    reserved1;
    uint:16    reserved2;    
    uint:32    offBits;
}

struct InfoHeader {
    uint:16          biSize;
}

struct BmpFile {
    FileHeader fileHeader;
    InfoHeader infoHeader;
}